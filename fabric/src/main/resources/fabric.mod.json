{
  "schemaVersion": 1,
  "id": "craftgr",
  "version": "${version}",
  "name": "CraftGR",
  "description": "Plays Gensokyo Radio in Minecraft with a cool HUD overlay",
  "authors": [
    "KabanFriends"
  ],
  "contributors": [],
  "contact": {
    "sources": "https://github.com/KabanFriends/CraftGR",
    "issues": "https://github.com/KabanFriends/CraftGR/issues"
  },
  "environment": "client",
  "license": "LGPL-3.0",
  "icon": "icon.png",
  "entrypoints": {
    "modmenu": [
      "io.github.kabanfriends.craftgr.fabric.ModMenuIntegration"
    ],
    "client": [
      "io.github.kabanfriends.craftgr.fabric.CraftGRFabric"
    ],
    "server": []
  },
  "mixins": [
    "craftgr-common.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.20.5-",
    "fabricloader": "*",
    "fabric-key-binding-api-v1": "*",
    "fabric-lifecycle-events-v1": "*",
    "fabric-rendering-v1": "*",
    "fabric-screen-api-v1": "*"
  },
  "recommends": {
    "yet_another_config_lib_v3": ">=3.4.1"
  },
  "custom": {
    "modmenu:clientsideOnly": true
  }
}